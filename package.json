{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "webpack",
    "clean": "rm -rf ./node_modules",
    "lint": "eslint --ext .js src ./",
    "lint:fix": "npm run lint -- --fix",
    "lint:style": "stylelint 'src/styles/**/*.js'",
    "format": "prettier '**/*.js'",
    "format:check": "npm run format -- --list-different",
    "format:fix": "npm run format -- --write",
    "setup": "yarn install && yarn flow-typed install",
    "start": "webpack-dev-server --inline --progress",
    "typecheck": "flow check",
    "test": "jest"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.9",
    "eslint": "^4.17.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.42.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "flow-bin": "^0.64.0",
    "flow-typed": "^2.3.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.1.4",
    "prettier": "^1.10.2",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.2.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "reselect": "^3.0.1",
    "ress": "^1.2.2",
    "styled-components": "^3.1.6"
  }
}
