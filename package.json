{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "webpack",
    "clean": "rm -rf ./node_modules",
    "format": "prettier '**/*.js'",
    "format:check": "npm run format -- --list-different",
    "format:fix": "npm run format -- --write",
    "start": "webpack-dev-server --inline --progress",
    "test": "jest"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.9",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.1.4",
    "prettier": "^1.10.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "reselect": "^3.0.1",
    "ress": "^1.2.2"
  }
}
